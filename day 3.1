For the given JSON iterate over all for loops (for, for in, for of, forEach)


var obj= [
    {Name:"Haritha", age:25, place:"chennai"},
    {Name:"Remifa", age:26, place:"chennai"},
    {Name: "Gokul", age:26,place:"chennai"},
]

// using for loop:
for(i=0; i<obj.length; i++){
    user=obj[i]
    console.log(`
        Name: ${user.Name}
        Age: ${user.age}
        place: ${user.place}
        `
    );
}


// using forIn loop:

for (var index in obj) {
    user=obj[index];
    console.log(`
        Name: ${user.Name}
        Age: ${user.age}
        place: ${user.place}
        `
    );

}

// using forOf loop:

for(var index of obj){
    console.log(`
        Name: ${index.Name}
        Age; ${index.age}
        place: ${index.place}
        `
        );
}

// using forEach loop:
obj.forEach((user)=> {
    console.log(`
        Name: ${user.Name}
        Age: ${user.age}
        place: ${user.place}
        `
    );

}



